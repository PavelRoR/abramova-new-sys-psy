#program
	padding 25px 0
.program-container
	padding 45px 520px 45px 45px
	background-image linear-gradient(to right, 	#fff 57%, transparent 82%), url('../img/images/program-img.jpg')
	background-repeat no-repeat
	background-position: center left
	background-size: cover
	box-shadow: 0px 4px 26.68px 2.32px rgba(9, 78, 162, 0.49);
.program-table
	width 100%
	display: table
	&-row
		display: table-row
	&-cell
		display: table-cell
		width 100%
		&:first-of-type
			width 75%
			padding 20px 20px 0 0
			border-right: 2px solid #000
		&:last-of-type
			width 25%
			padding 20px 20px 0 20px
		&-head
			border-bottom: 2px solid #000
			text-transform: uppercase
			font-family: GBd
			margin-bottom: 20px
			&:first-of-type
				padding 0 0 10px
			&:last-of-type
				padding 0 0 10px
				text-align: center
.table-title
	text-transform: uppercase
	span
		font-weight: bold
.table-list
	margin-top: 20px
	li
		list-style: none
		margin-left: 70px
		margin-bottom: 10px
		&:last-child
			margin-bottom: 0
.table-text-duration
	font-family: GI
	text-align: center